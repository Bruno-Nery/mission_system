stock GetMissionRewardText(missionid) {
    new str[128];
    new reward_money = 0, reward_exp = 0, reward_cop = 0, reward_criminal = 0, reward_paramedic = 0;
    GetMissionReward(missionid, reward_money,reward_exp,reward_cop, reward_criminal);
    if(reward_money > 0 && reward_criminal == 0 && reward_cop == 0 && reward_exp == 0) {
        format(str, sizeof(str), "+%i$", reward_money);
    }
    else if(reward_money > 0 && reward_criminal > 0 && reward_cop == 0 && reward_exp == 0) {
        format(str, sizeof(str), "+%i$, +%i criminal", reward_money, reward_criminal);
    }
    else if(reward_money > 0 && reward_criminal > 0 && reward_cop > 0 && reward_exp == 0) {
        format(str, sizeof(str), "+%i$, +%i criminal, +%i cop", reward_money, reward_criminal, reward_cop);
    }
    else if(reward_money > 0 && reward_criminal > 0 && reward_cop > 0 && reward_exp > 0) {
        format(str, sizeof(str), "+%i$, +%i criminal, +%i cop, +%i exp", reward_money, reward_criminal, reward_cop, reward_exp);
    }
    else if(reward_money == 0 && reward_criminal > 0 && reward_cop == 0 && reward_exp == 0) {
        format(str, sizeof(str), "+%i criminal", reward_criminal);
    }
    else if(reward_money == 0 && reward_criminal == 0 && reward_cop > 0 && reward_exp == 0) {
        format(str, sizeof(str), "+%i cop", reward_cop);
    }
    else if(reward_money == 0 && reward_criminal == 0 && reward_cop == 0 && reward_exp > 0) {
        format(str, sizeof(str), "+%i exp", reward_exp);
    }
    else if(reward_money == 0 && reward_criminal > 0 && reward_cop > 0 && reward_exp == 0) {
        format(str, sizeof(str), "+%i criminal, +%i cop", reward_criminal, reward_cop);
    }
    else if(reward_money == 0 && reward_criminal > 0 && reward_cop > 0 && reward_exp > 0) {
        format(str, sizeof(str), "+%i criminal, +%i cop, +%i exp", reward_criminal, reward_cop, reward_exp);
    }
    return str;
}

forward MissionComplete(playerid, missionid);
public MissionComplete(playerid, missionid) {
    if(mission[playerid] != missionid) {
        new str[128];
        format(str, sizeof(str), "MissionComplete error: %i/%i", missionid, mission[playerid]);
        SendClientMessage(playerid, COLOR_LIGHTBLUE, str);
        return 1;
    }
    new reward_money = 0, reward_exp = 0, reward_cop = 0, reward_criminal = 0, reward_paramedic = 0;
    GetMissionReward(missionid, reward_money,reward_exp,reward_cop, reward_criminal, reward_paramedic);
    GiveMissionReward(playerid, reward_exp, reward_cop, reward_criminal, reward_money, reward_paramedic);
    return 1;
}




