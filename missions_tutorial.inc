forward MissionTutorial(playerid, missionid);
public MissionTutorial(playerid, missionid) {
    new titleStr[128];
    new descStr[1024];

    format(descStr, sizeof(descStr), "{FFFFFF}%s", GetMissionContent(playerid, missionid));
    format(titleStr, sizeof(titleStr), "--- Mission %i: %s ---", missionid, GetMissionTitle(playerid, missionid));

    ShowPlayerDialog(playerid,10000,DIALOG_STYLE_MSGBOX,titleStr,descStr,"Ok","");
    return true;
}